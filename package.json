{
  "name": "stereo-hub",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "server-dev": "nodemon server/app.js --exec babel-node",
    "client-dev": "webpack-dev-server --config webpack/dev.js",
    "build": "rimraf dist && npm run build-client && npm run build-server",
    "build-client": "webpack --config webpack/prod.js",
    "build-server": "babel server -d dist/server",
    "run-server": "node server/app.js",
    "lint-client": "eslint client --fix",
    "lint-server": "eslint server --fix",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "author": "tom mendelson",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.4.0",
    "concurrently": "^3.4.0",
    "eslint": "^4.15.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^22.1.4",
    "jss-rtl": "^0.2.1",
    "nodemon": "^1.14.11",
    "typeface-roboto": "0.0.45"
  },
  "dependencies": {
    "bluetooth-serial-port": "^2.1.6",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.4",
    "multer": "^1.3.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "uuid": "^3.1.0"
  }
}
